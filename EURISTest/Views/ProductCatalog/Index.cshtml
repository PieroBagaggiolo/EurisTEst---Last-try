@model IEnumerable<EURISTest.Models.ProductCatalog>

@{
    ViewBag.Title = "Index";
}

<h2>Index Relazione Cataloghi Prodotti</h2>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Catalog.Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Product.Description)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Catalog.Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Product.Description)
        </td>
        <td>
            @Html.ActionLink("Modifica", "Edit", new { id = item.Id }) |
            @Html.ActionLink("Dettagli", "Details", new { id = item.Id }) |
            @Html.ActionLink("Elimina", "Delete", new { id = item.Id }) |
        </td>
    </tr>
}

</table>
